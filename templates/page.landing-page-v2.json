          }
        },
        "question_Ffq4tF": {
          "type": "question",
          "settings": {
            "title": "How long will it take to get my order?",
            "text": "<p>Our orders are typically dispatched within 1 business and take 1 to 2 days to arrive with Royal Mail.</p>",
            "highlighted_text": "none",
            "highlighted_scribble": "circle"
          }
        },
        "question_JWc9eb": {
          "type": "question",
          "settings": {
            "title": "Can I exchange a fragrance?",
            "text": "<p>Yes. If you want to exchange a fragrance, please contact our Customer Services team on our <a href=\"/pages/contact\" title=\"Contact\">contact page</a> or use our chatbot on the bottom right of your screen.</p>",
            "highlighted_text": "none",
            "highlighted_scribble": "circle"
          }
        }
